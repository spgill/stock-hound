{
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],

    "parser": "babel-eslint",

    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },

    "globals": {
        "_": true,
        "React": true
    },

    "rules": {
        "react/prop-types": 0
    },

    "plugins": [
        "react"
    ]
}
